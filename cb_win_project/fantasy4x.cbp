<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="fantasy4x" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="debug">
				<Option output="../target/debug/fantasy4x" prefix_auto="1" extension_auto="1" />
				<Option object_output="../obj/debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="release">
				<Option output="../target/release/fantasy4x" prefix_auto="1" extension_auto="1" />
				<Option object_output="../obj/release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wshadow" />
			<Add option="-Winit-self" />
			<Add option="-Wredundant-decls" />
			<Add option="-Wundef" />
			<Add option="-Wfloat-equal" />
			<Add option="-Wunreachable-code" />
			<Add option="-Wmissing-declarations" />
			<Add option="-Wmissing-include-dirs" />
			<Add option="-Wmain" />
			<Add option="-pedantic-errors" />
			<Add option="-pedantic" />
			<Add option="-Wfatal-errors" />
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-ansi -Werror" />
		</Compiler>
		<Unit filename="../src/main.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
